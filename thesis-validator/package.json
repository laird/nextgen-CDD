{
  "name": "thesis-validator",
  "version": "1.0.0",
  "description": "Agentic Commercial & Technical Diligence Research System for Private Equity",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:init": "tsx scripts/init-db.ts",
    "db:schema": "tsx scripts/create-schema.ts",
    "db:migrate": "tsx scripts/run-migrations.ts",
    "seed:skills": "tsx scripts/seed-skills.ts",
    "migrate:memory": "tsx scripts/migrate-memory.ts",
    "benchmark": "tsx scripts/benchmark.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "due-diligence",
    "private-equity",
    "vector-database",
    "ai-agents",
    "research"
  ],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.56",
    "@ai-sdk/google-vertex": "^3.0.92",
    "@ai-sdk/openai-compatible": "^1.0.29",
    "@anthropic-ai/sdk": "^0.30.0",
    "@anthropic-ai/vertex-sdk": "^0.14.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^13.0.2",
    "@fastify/jwt": "^8.0.1",
    "@fastify/multipart": "^9.3.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/websocket": "^10.0.1",
    "@google-cloud/aiplatform": "^5.14.0",
    "@google-cloud/local-auth": "^3.0.1",
    "ai": "^5.0.114",
    "bullmq": "^5.12.0",
    "cheerio": "^1.1.2",
    "dotenv": "^17.2.3",
    "fastify": "^4.28.1",
    "fastify-zod": "^1.4.0",
    "google-auth-library": "^9.14.0",
    "ioredis": "^5.4.1",
    "mammoth": "^1.11.0",
    "openai": "^4.67.0",
    "pdf-parse": "^2.4.5",
    "pg": "^8.12.0",
    "tesseract.js": "^6.0.1",
    "uuid": "^10.0.0",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.25.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^22.7.0",
    "@types/pdf-parse": "^1.1.5",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vitest/coverage-v8": "^2.1.1",
    "eslint": "^9.11.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.50.0",
    "vitest": "^2.1.1",
    "ws": "^8.18.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
